{
    "name": "roy404/utilities",
    "description": "A comprehensive collection of PHP utilities including Carbon, Logger, Storage, RateLimiter, Cache, and Session Handler, and more designed to enhance productivity and simplify development.",
    "keywords": [
        "php",
        "compiler"
    ],
    "autoload": {
        "psr-4": {
            "App\\Bootstrap\\Exceptions\\": "exceptions",
            "App\\Bootstrap\\Handler\\": "core/handler",
            "App\\Bootstrap\\Helper\\": "core/helper",
            "App\\Utilities\\Handler\\": "utilities/handler",
            "App\\Utilities\\Blueprints\\": "utilities/blueprints",
            "App\\Utilities\\": "utilities",
            "App\\Bootstrap\\": "core",
            "App\\Http\\": "http"
        },
        "files": [
            "Standard.php"
        ]
    },
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Robroy B Canales",
            "email": "canales.robroy123@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "roy404/blades": "^1.0",
        "roy404/request": "^3.0",
        "roy404/artisan": "^1.0",
        "roy404/eloquent": "^1.1.3",
        "phpmailer/phpmailer": "^6.9",
        "roy404/routes": "^4.1",
        "aws/aws-sdk-php": "^3.356"
    }
}
