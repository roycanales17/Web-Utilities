{
    "name": "roy404/utilities",
    "description": "A comprehensive collection of PHP utilities including Carbon, Logger, Storage, RateLimiter, Cache, and Session Handler, designed to enhance productivity and simplify development.",
    "keywords": [
        "php",
        "compiler"
    ],
    "autoload": {
        "psr-4": {
            "App\\Utilities\\Blueprints\\": "src/blueprints",
            "App\\Utilities\\": "src",
            "App\\Http\\": "http"
        },
        "files": [
            "Standard.php"
        ]
    },
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Robroy B Canales",
            "email": "canales.robroy123@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "roy404/blades": "^1.0",
        "roy404/request": "^3.0",
        "roy404/artisan": "^1.0",
        "roy404/eloquent": "^1.0",
        "phpmailer/phpmailer": "^6.9",
        "roy404/schema": "^1.0"
    }
}
